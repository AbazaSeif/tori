<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<span class="topic" ui:field='topic'>
			<g:Label styleName="followtopic" ui:field='follow' />
			<a class="topicname" ui:field='topicName' />
			<span class="threadicons" />
		</span>
		<span class="started" ui:field='started'>
			<span class="startedby" ui:field='startedBy' />
			<span class="startedtime" ui:field='startedTime' />
		</span>
		<span class="replycountwrapper" ui:field='replyCountWrapper'>
			<span class="replycount" ui:field='replyCount' />
		</span>
		<a class="latest" ui:field='latest' href="">
			<span class="latestby" ui:field='latestBy' />
			<span class="latesttime" ui:field='latestTime' />
		</a>
		<g:SimplePanel styleName="settings" ui:field="settings" />
	</g:HTMLPanel>
</ui:UiBinder> 